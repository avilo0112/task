def is_correct_brackets(text):
    while '()' in text or '[]' in text:
        text = text.replace('()', '')
        text = text.replace('[]', '')

    return not text


print(is_correct_brackets('[((())()(())]]'))  # False
print(is_correct_brackets('[((())()(()))]')) # убрала одну ']' - закрывающую и добавила одну ')' - закрывающую перед ']'
